<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sp.app.dao.Data_dictionaryDao">

     <resultMap type="Data_dictionary" id="BaseResultMap">
         <id property="id" column="id"/>
         <result property="typeCode" column="typeCode"/>
         <result property="typeName" column="typeName"/>
         <result property="valueId" column="valueId"/>
         <result property="valueName" column="valueName"/>
         <result property="createdBy" column="createdBy"/>
         <result property="creationDate" column="creationDate"/>
         <result property="modifyBy" column="modifyBy"/>
         <result property="modifyDate" column="modifyDate"/>
     </resultMap>
     
     <resultMap type="Data_dictionary" id="FullResultMap" extends="BaseResultMap">
         <collection property="backend_user" resultMap="cn.sp.app.dao.Backend_userDao.BaseResultMap"/>
     </resultMap>
     
     <select id="getAll" resultMap="FullResultMap">
     	select * from data_dictionary
     </select>
</mapper>